{
    "name": "orchestra/tenanti",
    "description": "Tenant based Database Schema Manager for Laravel",
    "keywords": [
        "laravel",
        "orchestral",
        "orchestra-platform",
        "multi-tenant",
        "schema",
        "migration"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Mior Muhammad Zaki",
            "email": "crynobone@gmail.com",
            "homepage": "https://github.com/crynobone"
        }
    ],
    "autoload": {
        "psr-4": {
            "Orchestra\\Tenanti\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Orchestra\\Tenanti\\Tests\\": "tests/"
        },
        "classmap": [
            "tests/Kernel.php"
        ]
    },
    "require": {
        "php": "^8.1",
        "illuminate/console": "^10.0",
        "illuminate/filesystem": "^10.0",
        "illuminate/support": "^10.0",
        "enriquejlicona/model": "^8.0",
        "enriquejlicona/support": "^8.0"
    },
    "require-dev": {
        "laravel/tinker": "^2.8",
        "nunomaduro/larastan": "^2.4",
        "orchestra/canvas": "^6.1",
        "orchestra/testbench": "^8.0"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "6.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev"
}
